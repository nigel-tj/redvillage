/ start gallery
#gallery.text-center
  .container-fluid
    .col-md-12
      .row.text-center
        .tabs.tabs-style-linebox.wow.fadeInUp{"data-wow-duration" => "2s"}
          %nav
            %ul.filters
              %li.hvr-sweep-to-top.active.filter{"data-filter" => "all"}
                %span All
              %li.hvr-sweep-to-top.filter{"data-filter" => ".girlsPub"}
                %span #CCW
              %li.hvr-sweep-to-top.filter{"data-filter" => ".danceParty"}
                %span #MM
              %li.hvr-sweep-to-top.filter{"data-filter" => ".videos"}
                %span #RedCupVillage
 
      .row.gallery.text-center
        .col-md-12.noPadding
          .gallery-photo-grid
            .gallery-photos
              .gallery-photos-thumb
                - for insta_tag in @results
                  .gallery-photo-item.mix.danceParty.wow.fadeIn
                    %a.pretty-photo{"data-gal" => "prettyPhoto[gallery-photo-thumb]", :href => "#{insta_tag.images.standard_resolution.url}"}
                      .photo-thumb
                        %img{:alt => "image-1", :src => "#{insta_tag.images.standard_resolution.url}"}
                      .overlay
                        %span.gallery-icon
                          %i.fa.fa-search-plus.fa-2x
/ end gallery
