      .item.active
        %img{:src => "https://fbcdn-sphotos-d-a.akamaihd.net/hphotos-ak-xpf1/v/t1.0-9/11164703_1599834523633120_4281180496812680020_n.png?oh=0ecbf41d58be6f3db2d80ce72b049d9b&oe=560A21A3&__gda__=1442828444_d544c8fefef29f193e828a8f71f74864"}/
        .container
          // .carousel-caption
