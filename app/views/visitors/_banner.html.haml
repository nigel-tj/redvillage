

.container-fluid.hero
  / Carousel
  #hero-carousel.carousel.slide{"data-ride" => "carousel"}
    / Indicators
    %ol.carousel-indicators.row
      %li.col-sm-4.message-1.active{"data-slide-to" => "0", "data-target" => "#hero-carousel"}
        .caption CAPTION 1
      %li.col-sm-4.message-2{"data-slide-to" => "1", "data-target" => "#hero-carousel"}
        .caption CAPTION 2
      %li.col-sm-4.message-3{"data-slide-to" => "2", "data-target" => "#hero-carousel"}
        .caption CAPTION 3
    / Wrapper for slides
    .carousel-inner
      .item.active
        .carousel-card{:style => "background-image: url(\"https://scontent-ams2-1.xx.fbcdn.net/hphotos-xat1/v/t1.0-9/11246478_1421750491481057_8925542422565878626_n.jpg?oh=00c3b0176a332d1d61c324a2f26fa19d&oe=55F7A5E3\"); background-size: cover;"}
        .carousel-caption
          %h1.message-1 Title 1
          %p
            %a.btn.btn-lg.message-1{:href => "#"} Link to title 1
      .item
        .carousel-card{:style => "background-image: url(\"https://scontent-ams3-1.xx.fbcdn.net/hphotos-xat1/v/t1.0-9/11143436_1607120386237867_3355442846362451173_n.png?oh=2777135c12af1be1c7313519677f0b1b&oe=560BBB35\"); background-size: cover;"}
        .carousel-caption
          %h1.message-2 Title 2!
          %p
            %a.btn.btn-lg.message-2{:href => "#"} Link to title 2
      .item
        .carousel-card{:style => "background-image: url(\"https://scontent-ams2-1.xx.fbcdn.net/hphotos-xpa1/v/t1.0-9/11231685_100995740239660_3897933186335868178_n.jpg?oh=4771422f0bc7d2885ed0efe830ce29cb&oe=560D3AE7\"); background-size: cover;"}
        .carousel-caption
          %h1.message-3 Title 3?
          %p
            %a.btn.btn-lg.message-3{:href => "#"} Link to title 1
