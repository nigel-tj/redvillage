/ Start bannerd
#banner.module.parallax.parallax-header
  .container
    .row
      .banner-inner
        #header-slider.owl-carousel
          .item.col-xs-12
            %h1.title.title-lines
              %span 10 March, 2016
            %h3.subtitle Live Concert in France
            %a.btn.scroll.btn-concert-register{:href => "#reservation"} Register
            %a.btn.btn-concert-play{"data-gal" => "prettyPhoto", :href => "http://vimeo.com/8245346"} Watch Video
          .item
            .col-sm-5.col-lg-4.col-xs-12
              .concert-registration
                %form.form-inline
                  .form-group
                    %label.sr-only{:for => "inputName"} Name
                    %input#inputName.form-control{:placeholder => "Name", :required => "", :type => "text"}
                  .form-group
                    %label.sr-only{:for => "inputEmail"} Email
                    %input#inputEmail.form-control{:placeholder => "Email", :required => "", :type => "password"}
                  .form-group
                    %label.sr-only{:for => "inputPhone"} Phone
                    %input#inputPhone.form-control{:placeholder => "Phone", :required => "", :type => "password"}
