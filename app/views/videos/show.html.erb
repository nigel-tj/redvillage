<div id="services">
  <div class="container">
    <div class="row">
    </div>
  </div>
</div>
<div class="container">
  <h1>New video</h1>
  <%= form_for @video do |f| %>
    <%= render 'shared/errors', object: @video %>
    <div class="form-group">
      <%= f.label :link, class: "control-label col-sm-2" %>
      <div class="col-sm-10">
        <%= f.text_field :link, class: 'form-control', required: true %>
        <span class="help-block">A link to the video on YouTube.</span>
      </div>
    </div>
    <div class="form-group">
      <%= f.label :category, class: "control-label col-sm-2" %>
      <div class="col-sm-10">
        <%= f.submit class: 'btn btn-primary' %>
      </div>
    </div>
  <% end %>
</div>

