#services
  .container
    .row
.container
  %h1 New video
  = form_for @video do |f|
    = render 'shared/errors', object: @video
    .form-group
      = f.label :link
      = f.text_field :link, class: 'form-control', required: true
      %span.help-block A link to the video on YouTube.
      = f.select "category", [["Select category","none"],["Bangers","bangers"], ["Village hits","villageHits"], ["Upcoming","upcoming"]]
    = f.submit class: 'btn btn-primary'
